<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>English</string>
	<key>CFBundleExecutable</key>
	<string>GrandPerspective</string>
	<key>CFBundleIconFile</key>
	<string>GrandPerspective.icns</string>
	<key>CFBundleIdentifier</key>
	<string>net.sourceforge.grandperspectiv</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleSignature</key>
	<string>GrPe</string>
	<key>CFBundleVersion</key>
	<string>11</string>
	<key>CFBundleShortVersionString</key>
	<string>0.9.10</string>
	<key>LSHasLocalizedDisplayName</key>
	<true/>
	<key>NSMainNibFile</key>
	<string>MainMenu</string>
	<key>NSPrincipalClass</key>
	<string>NSApplication</string>
    <key>CFBundleHelpBookFolder</key>
    <string>GrandPerspectiveHelp</string>
    <key>CFBundleHelpBookName</key>
    <string>GrandPerspective Help</string>
    <key>GPApplicationDefaults</key>
    <dict>
        <key>fileDeletionTargets</key>
        <string>delete nothing</string>
        <key>confirmFileDeletion</key>
        <true/>
        <key>confirmFolderDeletion</key>
        <true/>
        <key>defaultColorPalette</key>
        <string>CoffeeBeans</string>
        <key>defaultColorMapping</key>
        <string>folder</string>
        <key>fileSizeMeasure</key>
        <string>logical</string>        
    </dict>
    <key>GPDefaultFileItemTests</key>
    <array>
		<dict>
			<key>name</key>
			<string>Tiny files</string>
			<key>class</key>
			<string>ItemSizeTest</string>
			<key>upperBound</key>
			<integer>1024</integer>
		</dict>
		<dict>
			<key>name</key>
			<string>Small files</string>
			<key>class</key>
			<string>ItemSizeTest</string>
			<key>lowerBound</key>
			<integer>1024</integer>
			<key>upperBound</key>
			<integer>10240</integer>
		</dict>
		<dict>
			<key>name</key>
			<string>Medium files</string>
			<key>class</key>
			<string>ItemSizeTest</string>
			<key>lowerBound</key>
			<integer>10240</integer>
			<key>upperBound</key>
			<integer>1048576</integer>
		</dict>
		<dict>
			<key>name</key>
			<string>Large files</string>
			<key>class</key>
			<string>ItemSizeTest</string>
			<key>lowerBound</key>
			<integer>1048576</integer>
			<key>upperBound</key>
			<integer>104857600</integer>
		</dict>
		<dict>
			<key>name</key>
			<string>Huge files</string>
			<key>class</key>
			<string>ItemSizeTest</string>
			<key>lowerBound</key>
			<integer>104857600</integer>
		</dict>
		<dict>
			<key>name</key>
			<string>Images</string>
			<key>class</key>
			<string>ItemNameTest</string>
			<key>stringTest</key>
			<dict>
				<key>class</key>
				<string>StringSuffixTest</string>
				<key>caseSensitive</key>
				<true/>
				<key>matches</key>
				<array>
					<string>.jpg</string>
					<string>.jpeg</string>
					<string>.png</string>
					<string>.gif</string>
				</array>
			</dict>
		</dict>
		<dict>
			<key>name</key>
			<string>Music</string>
			<key>class</key>
			<string>ItemNameTest</string>
			<key>stringTest</key>
			<dict>
				<key>class</key>
				<string>StringSuffixTest</string>
				<key>caseSensitive</key>
				<true/>
				<key>matches</key>
				<array>
					<string>.mp3</string>
					<string>.wav</string>
				</array>
			</dict>
		</dict>
		<dict>
			<key>name</key>
			<string>Version control</string>
			<key>class</key>
			<string>ItemPathTest</string>
			<key>stringTest</key>
			<dict>
				<key>class</key>
				<string>StringContainmentTest</string>
				<key>caseSensitive</key>
				<true/>
				<key>matches</key>
				<array>
					<string>/CVS/</string>
					<string>/.svn/</string>
				</array>
			</dict>
		</dict>
	</array>
</dict>
</plist>
